{
    "window": {
        "title": "test !",
        "width": 640,
        "height": 480,
        "fullscreen": false,
        "mouse_cursor": true
    },

    "keys": {
        "exit": ["Gosu::GP_BUTTON_4", "Gosu::KB_ESCAPE"],
        "validation_key": ["Gosu::GP_BUTTON_0", "Gosu::KB_E"],
        "up": ["Gosu::GP_UP", "Gosu::KB_W"],
        "down": ["Gosu::GP_DOWN", "Gosu::KB_S"],
        "right": ["Gosu::GP_RIGHT", "Gosu::KB_D"],
        "left": ["Gosu::GP_LEFT", "Gosu::KB_A"],
        "run": ["Gosu::GP_BUTTON_2", "Gosu::KB_LEFT_SHIFT"],
        "cancel": ["Gosu::GP_BUTTON_3", "Gosu::KB_LEFT_CONTROL"],
        "menu": ["Gosu::GP_BUTTON_1", "Gosu::KB_TAB"],
        "aim": ["Gosu::GP_BUTTON_10", "Gosu::KB_SPACE"]
    }
}